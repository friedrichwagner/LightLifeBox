<UserControl x:Class="ModernUIApp2.Pages.LEDTest2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Content/LEDTest2/Step1.xaml" SnapsToDevicePixels="False">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Step 1: Header Data" Source="/Content/LEDTest2/Step1.xaml"/>
                <mui:Link DisplayName="Step 2: Test LED Modules" Source="/Content/LEDTest2/Step7.xaml" />
                <mui:Link DisplayName="Step 3: List All" Source="/Content/LEDTest2/StepListAll.xaml" />
