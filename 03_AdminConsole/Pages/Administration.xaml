<UserControl x:Class="LightLifeAdminConsole.Pages.Administration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" PreviewKeyUp="UserControl_PreviewKeyUp">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Content/Admin/Roles.xaml#LLBox" SnapsToDevicePixels="False">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Boxen" Source="/Content/Admin/Roles.xaml#LLBox"/>
                <mui:Link DisplayName="Szenen" Source="/Content/Admin/Roles.xaml#LLScene" />
                <mui:Link DisplayName="----------" />
                <mui:Link DisplayName="Sehtest" Source="/Content/Admin/Roles.xaml#LLDeltaTest" />
                <mui:Link DisplayName="Test Sequenzen" Source="/Content/Admin/Roles.xaml#LLTestSequenceHead" />               
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
