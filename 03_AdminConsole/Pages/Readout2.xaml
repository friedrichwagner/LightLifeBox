<UserControl x:Class="ModernUIApp2.Pages.Readout2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" PreviewKeyUp="UserControl_PreviewKeyUp">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Content/Readout2/Step1_Header.xaml" SnapsToDevicePixels="False">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Step 1: Header Data" Source="/Content/Readout2/Step1_Header.xaml"/>
                <mui:Link DisplayName="Step 2: Readout/Print" Source="/Content/Readout2/Step2_Readout.xaml" />
                <mui:Link DisplayName="Step 3: List All/Export" Source="/Content/Readout2/StepX_ListAll.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
